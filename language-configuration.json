{
    "comments": {
        // symbol used for single line comment in MASM
        "lineComment": ";"
    },
    // symbols used as brackets
    "brackets": [
        ["<", ">"],
        ["[", "]"],
        ["(", ")"]
    ],
    // symbols that are auto closed when typing
    "autoClosingPairs": [
        { "open": "<", "close": ">" },
        { "open": "[", "close": "]" },
        { "open": "(", "close": ")" },
        { "open": "\"", "close": "\"" },
        { "open": "'", "close": "'" }
    ],
    // symbols that can be used to surround a selection
    "surroundingPairs": [
        { "open": "<", "close": ">" },
        { "open": "[", "close": "]" },
        { "open": "(", "close": ")" },
        { "open": "\"", "close": "\"" },
        { "open": "'", "close": "'" }
    ]
}
